<?xml version='1.0' encoding='utf-8'?>
<widget id="com.parseapp.nyankitty" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>NyanKitty</name>
    <description>
        Like FlappyBird? Then you'll love this cat :3
    </description>
    <author email="kearlsaint@gmail.com" href="http://twitter.com/kearlsaint">
        @kearlsaint
    </author>
    <feature name="http://api.phonegap.com/1.0/device" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="true" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <icon src="icon.png" />
    <icon gap:density="ldpi" gap:platform="android" src="ios/57.png" />
    <icon gap:density="mdpi" gap:platform="android" src="ios/57.png" />
    <icon gap:density="hdpi" gap:platform="android" src="ios/72.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="ios/114.png" />
    <icon gap:platform="blackberry" src="ios/72.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="ios/72.png" />
    <icon gap:platform="ios" height="57" src="ios/57.png" width="57" />
    <icon gap:platform="ios" height="72" src="ios/72.png" width="72" />
    <icon gap:platform="ios" height="114" src="ios/114.png" width="114" />
    <icon gap:platform="ios" height="144" src="ios/144.png" width="144" />
    <icon gap:platform="webos" src="ios/57.png" />
    <icon gap:platform="winphone" src="ios/57.png" />
    <icon gap:platform="winphone" gap:role="background" src="ios/57.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="ios/splash/apple-touch-startup-image-320x460.jpg" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="ios/splash/apple-touch-startup-image-640x920.jpg" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="ios/splash/apple-touch-startup-image-640x920.jpg" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="ios/splash/apple-touch-startup-image-640x920.jpg" />
    <gap:splash gap:platform="blackberry" src="ios/splash/apple-touch-startup-image-320x460.jpg" />
    <gap:splash gap:platform="ios" height="480" src="ios/splash/apple-touch-startup-image-320x460.jpg" width="320" />
    <gap:splash gap:platform="ios" height="960" src="ios/splash/apple-touch-startup-image-640x920.jpg" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="ios/splash/apple-touch-startup-image-768x1004.jpg" width="768" />
    <gap:splash gap:platform="ios" height="768" src="ios/splash/apple-touch-startup-image-748x1024.jpg" width="1024" />
    <gap:splash gap:platform="winphone" src="ios/splash/ios/splash/apple-touch-startup-image-640x920.jpg" />
    <access origin="http://127.0.0.1*" />
</widget>
